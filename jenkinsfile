//Sample jenkinsfile script
//CI-CD Build pipeline
pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                println "Build log"
            }
        }
        stage('Deploy') { 
            steps {
                println "Deploy log"
            }
        }
        stage('Test') { 
            steps {
                println "Test log"
            }
        }
    }
}
